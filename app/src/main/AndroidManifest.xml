<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ikhlasapp.consciouseyes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ConsciousEyes">
        <activity android:name="com.ikhlasapp.consciouseyes.InviteFriend"></activity>
        <activity android:name="com.ikhlasapp.consciouseyes.TrackProgress" />

        <activity android:name="com.ikhlasapp.consciouseyes.ViewHistory" />
        <activity android:name="com.ikhlasapp.consciouseyes.AddFriend" />
        <activity android:name="com.ikhlasapp.consciouseyes.HomeActivity" />
        <activity android:name="com.ikhlasapp.consciouseyes.LoginActivity" />
        <activity android:name="com.ikhlasapp.consciouseyes.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>